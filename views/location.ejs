<body>
	<header>
		<a href="/" class="logo">Matcha</a>
		<a href="/"><img src="./icons/logo.ico" class="logo-icon"></a>
	</header>
	<div class="profile-container">
		<div class="form-wrapper">
			<div class="left" style="
				background: url('./icons/index.jpg'); 
				background-size: cover; 
				background-position: center center;
				background-repeat: no-repeat;" >
				<div id="mapid"></div>
				<div class="data">
					<h2>Location</span></h2>
					<p>Location Location Location. This is one of the way we find you the perfect match</p>
				</div>
			</div>
			<div class="right">
				<div class="profile-container">
					<div class="form-wrapper">
						<div class="right">
							<h2>Location</h2>
							<form method="POST">
								<div class="input-container">
									<div class="gender-wrapper">
										<div class="interest-wrapper">
											<input class="input-interest" type="text" name="Location1" placeholder="Location">
											<input class="inactive" type="text" name="Location2">
											<div class="btn-container">		
												<button class="submit-btn" id="sign-in"> 
													submit
												</button>
											</div>
										</div>
									</div>
								</div>
								
								
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<footer>
		<p>pdonga@2019</p>
	</footer>
	<script 
			src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js"
